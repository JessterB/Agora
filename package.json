  "name": "agora",
  "version": "3.0.0",
  "data-file": "syn13363290",
  "data-version": "40",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"npm:dev:server\" \"npm:dev:app\"",
    "dev:app": "webpack serve --config config/webpack.dev.js --port 3000 --progress",
    "dev:server": "webpack watch --config config/webpack.server.js --mode development --progress",
    "build": "npm run build:server & npm run build:app",
    "build:app": "npm run clean:app && webpack build --config config/webpack.prod.js --mode production --progress",
    "build:server": "npm run clean:server && webpack build --config config/webpack.server.js --mode production --progress",
    "start": "node --max-old-space-size=2048 ./dist/server.js",
    "stop": "pkill -SIGINT -f ./dist/server.js",
    "lint": "eslint \"./src/**/*.ts\"",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "karma start config/karma.conf.js",
    "test:watch": "npm run lint && npm run test:unit -- --auto-watch --no-single-run",
    "test:e2e": "concurrently --kill-others \"npm run start\" \"npm run cypress:run && npm run stop\"",
    "ci:zip": "zip /tmp/agora-app.zip -qr9 dist package.json package-lock.json LICENSE",
    "clean": "npm run clean:app-and-server && npm run clean:testing && npm run clean:data && npm run clean:packages",
    "clean:app": "rimraf \"./dist/**/!(server.js)\"",
    "clean:server": "rimraf ./dist/server.js",
    "clean:app-and-server": "rimraf ./dist",
    "clean:testing": "rimraf ./testing/spec-build coverage",
    "clean:data": "rimraf ../data/*.json & rimraf ../data/team_images/*.jpg",
    "clean:packages": "npm cache clean --force && rimraf ./node_modules",
    "mongo:import": "sh ./scripts/mongo-import.sh",
    "mongo:create:indexes": "mongo --host localhost ./src/server/scripts/createIndexes.js",
    "mongo:start": "sh ./scripts/start-mongo.sh '/var/lib/mongodb/'",
    "mongo:start:windows": "sh ./scripts/start-mongo.sh 'C:\\data\\db'",
    "data:local": "npm run clean:data && sh ./scripts/get-data-local.sh",
    "data:local-aws": "npm run clean:data && sh ./scripts/get-data-local-aws.sh",
    "data:aws": "npm run clean:data && sh ./scripts/get-data-aws.sh",
    "cypress": "cypress",
    "cypress:run": "cypress run --config-file ./config/cypress.config.js"
  },
  "pre-commit": [
    "test"
  ],
