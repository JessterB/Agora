    "assets-webpack-plugin": "^3.5.1",
    "awesome-typescript-loader": "^3.3.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "bootstrap": "~4.0.0",
    "codelyzer": "^4.2.1",
    "colorbrewer": "^1.1.0",
    "copy-webpack-plugin": "^4.5.0",
    "cross-env": "^5.1.3",
